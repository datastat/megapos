            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "laravelcollective/html": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
